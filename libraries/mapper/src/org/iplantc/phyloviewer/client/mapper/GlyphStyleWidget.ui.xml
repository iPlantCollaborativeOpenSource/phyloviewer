<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    
  </ui:style>
  <g:HTMLPanel>
  	<g:Grid>
  		<g:row>
  			<g:cell>Stroke Color</g:cell>
  			<g:customCell>
  				<g:TextBox visibleLength="7" ui:field="strokeColorField" />
  			</g:customCell>
  		</g:row>
  		<g:row>
  			<g:cell>Fill Color</g:cell>
  			<g:customCell>
  				<g:TextBox visibleLength="7" ui:field="fillColorField" />
  			</g:customCell>
  		</g:row>
  		<g:row>
  			<g:cell>Line Width</g:cell>
  			<g:customCell>
  				<g:DoubleBox visibleLength="7" ui:field="lineWidthField"/>
  			</g:customCell>
  		</g:row>
  	</g:Grid>
  </g:HTMLPanel>
</ui:UiBinder> 